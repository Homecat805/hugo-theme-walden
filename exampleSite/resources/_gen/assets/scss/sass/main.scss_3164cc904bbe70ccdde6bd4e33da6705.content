  align-items: center;
  justify-self: end;
  padding: .5rem 1rem;
  font-size: 1.375rem; }

#menu {
  grid-column-start: 1;
  grid-column-end: 3;
  grid-row: 2;
  align-items: start;
  justify-self: center;
  display: none;
  width: 100%; }

a {
