  color: var(--baseof-ft); }

#header {
  grid-column: 1;
  grid-row: 1; }

#necker {
  grid-column: 1;
  grid-row: 2;
  background-color: var(--stager-bg);
  color: var(--stage-ft); }

#stager {
  grid-column: 1;
  grid-row: 3;
  background-color: var(--stager-bg);
  color: var(--stage-ft); }

#footer {
  grid-column: 1;
  grid-row: 4; }

.container-fluid {
  width: 100%; }

